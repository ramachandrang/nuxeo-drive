HKCR
{
	NoRemove CLSID
	{
		ForceRemove {6780E873-C33D-4498-AC3B-694221A87962} = s 'Icon Overlay In Progress Handler for Cloud Portal Office Desktop'
		{
			InprocServer32 = s '%MODULE%'
			{
				val ThreadingModel = s 'Apartment'
			}
			Version = s '1.0'
		}
	}
}

HKLM
{
	NoRemove Software
	{
		NoRemove Microsoft
		{
			NoRemove Windows
			{
				NoRemove CurrentVersion
				{
					NoRemove Explorer
					{
						NoRemove ShellIconOverlayIdentifiers
						{
							ForceRemove 2CpoDesktopInProgress = 
								s '{6780E873-C33D-4498-AC3B-694221A87962}'
						}
					}
				}
			}
		}
	}
}
