\par 
\par \hich\af40\dbch\af31505\loch\f40   . ./too\hich\af40\dbch\af31505\loch\f40 ls/posix/integration_env.sh
\par \hich\af40\dbch\af31505\loch\f40   nosetests nxdrive
\par 
\par \hich\af40\dbch\af31505\loch\f40 .. _pip: http://www.pip-installer.org/
